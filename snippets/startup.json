{
    "epicsEnvSet": {
        "prefix": "epics.epicsEnvSet",
        "body": "epicsEnvSet(${1:MACRO}, \"${2:MACRO_VALUE}\")${0}"
    },
    "require": {
        "prefix": "epics.require",
        "body": "require\t${1:module}, ${2:version}${0}"
    },
    "dbLoadTemplate": {
        "prefix": "epics.dbLoadTemplate",
        "body": "dbLoadTemplate(${1:file})${0}"
    },
    "dbLoadRecords": {
        "prefix": "epics.dbLoadRecords",
        "body": "dbLoadRecords(${1:file})${0}"
    }
}
